prog6 () : proc
      r : byte [6];
      s : byte [6];
      i : int;
   {
      r[0] = 'd';
      r[1] = 's';
      r[2] = 'a';
      r[3] = '\0';
      writeString(r);
      i = 0;
      while (i < 3) {
         s[i] = r[3-i-1];
         i = i+1;
      }
      s[i] = '\0';
      writeString(s);
   }

