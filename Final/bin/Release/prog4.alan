prog4 () : proc
   i : int;
   n : int;
   {
      writeString("Give me a number:");
      writeString(" ");
      n = readInteger();
      i = 1;
      while (i <= n) {
         if (i % 2 == 1) {
            writeInteger(i);
            writeChar(shrink(10));
         }
         i = i + 1;
      }
   }
